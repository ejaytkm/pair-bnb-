<h3> Sign Up here <h3> 
<%= form_for :user,url: "/users" ,method: "post" do |f| %>
  <%= f.label :username %>:
  <%= f.text_field :username %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.submit %>
<% end %>